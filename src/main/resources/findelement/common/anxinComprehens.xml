			<field name="idCHolder" byXpath="//input[@placeholder='请输入投保人的身份证号']" type="input"/>
			<field name="telCHolder" byXpath="//input[@placeholder='请输入投保人的手机号']" type="input"/>
			<field name="emailCHolder" byXpath="//input[@placeholder='用于接收电子保单邮件']" type="input"/>
			
			<field name="shebaoCBtn" byXpath="//div[@id='app']/div[3]/div/div[2]/div/div/div[3]/div/div[3]/div/div/div[1]" type="button"/>
			<field name="gouxuanCBtn" byXpath="//div[@id='app']/div[3]/div/span/i" type="button"/>
					
			<!-- 立即投保 -->
			<field name="lijiCBtn" byXpath="//div[text()='立即投保']" type="button"/>
			
		</page>
